[$HOME/.config/vcsh/repo.d/vim.git]
order = 10
# NOTE: put the correct url for your repository
checkout = vcsh clone -b vim https://github.com/DNajjarzade/vcsh_mr_template.git vim && \
           vcsh vim branch --set-upstream-to=origin/vim vim
           vcsh vim checkout -b vim && \
           vcsh vim branch --track vim origin/vim
